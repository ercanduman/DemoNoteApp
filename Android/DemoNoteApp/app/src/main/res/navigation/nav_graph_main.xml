<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/NotesFragment">

    <fragment
        android:id="@+id/NotesFragment"
        android:name="com.enbcreative.demonoteapp.ui.main.notes.NotesFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_note_list">

        <action
            android:id="@+id/action_NotesFragment_to_NoteDetailsFragment"
            app:destination="@id/NoteDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/NoteDetailsFragment"
        android:name="com.enbcreative.demonoteapp.ui.main.NoteDetailsFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_note_details">
        <action
            android:id="@+id/action_NoteDetailsFragment_to_NoteFragment"
            app:destination="@id/NotesFragment" />
    </fragment>
</navigation>
