<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/LoginFragment">

    <fragment
        android:id="@+id/LoginFragment"
        android:name="com.enbcreative.demonoteapp.ui.auth.login.LoginFragment"
        android:label="@string/fragment_label_login"
        tools:layout="@layout/fragment_login">

        <action
            android:id="@+id/action_LoginFragment_to_SignUpFragment"
            app:destination="@id/SignUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/SignUpFragment"
        android:name="com.enbcreative.demonoteapp.ui.auth.signup.SignUpFragment"
        android:label="@string/fragment_label_sign_up"
        tools:layout="@layout/fragment_sign_up" />
</navigation>
